include ../common.jade

#image-build

script(type='text/javascript').
    window.csrfToken = "#{csrf_token}";
    window.profileId = "#{profileId}";
    window.hostId = "#{hostId}";
    window.version = "#{version}";
    window.timezone = "#{h.renderTimezone()}";
    window.localTime = "#{h.renderLocalTime()}";
    window.actionChains = !{actionChains};
    window.UTCTime = "#{h.utcDateTime()}";
    window.serverTime = "#{h.serverDateTime()}";
    window.userTime = "#{h.renderLocalTime()}";

script(type='text/javascript').
    spaImportReactPage('images/image-build')
        .then(function(module) { module.renderer('image-build') });
