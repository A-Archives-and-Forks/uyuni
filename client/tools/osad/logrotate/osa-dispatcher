# logrotation file for RHN Satellite's osa-dispatcher

/var/log/rhn/osa-dispatcher.log {
    weekly
    rotate 5
    copytruncate
    compress
    notifempty
    missingok
    size 10M
    su wwwrun www
}

/var/log/rhn/oracle/osa-dispatcher/sqlnet.ora {
    weekly
    rotate 5
    copytruncate
    compress
    notifempty
    missingok
    size 10M
}
