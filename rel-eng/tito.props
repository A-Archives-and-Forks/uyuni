[globalconfig]
default_builder = tito.builder.Builder
default_tagger = tito.susetagger.SUSETagger
changelog_with_email = 0
changelog_do_not_remove_cherrypick = 0

[obs]
obs_projects = Devel:Galaxy:Manager:Head
obs_options = -A https://api.suse.de

[Devel:Galaxy:Manager:Head]
disttag =
blacklist=heirloom-pkgtools oracle-server-admin oracle-server-scripts rhnclient smartpm jabberd-selinux-workaround jabberd-selinux oracle-rhnsat-selinux oracle-selinux oracle-xe-selinux spacewalk-monitoring-selinux spacewalk-proxy-selinux spacewalk-selinux cx_Oracle apt-spacewalk perl-DBD-Oracle msv-workaround spacewalk-jpp-workaround spacewalk-oro-compat

